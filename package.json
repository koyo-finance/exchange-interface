		"generate:typechain:util": "typechain --target ethers-v5 --always-generate-overloads --discriminate-types --out-dir ./src/types/contracts/util './src/constants/abis/util/*.json'",
		"generate:graphql": "graphql-codegen --config codegen.yml -r dotenv/config",
		"postinstall": "yarn generate:typechain:exchange && yarn generate:typechain:koyo && yarn generate:typechain:util && yarn generate:graphql"
	},
